{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Build todos os módulos (skip tests)",
      "type": "shell",
      "command": "mvn clean package -DskipTests -T 1",
      "options": {
        "cwd": "${workspaceFolder}/rsdesenvolvimento-api"
      },
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "presentation": {
        "reveal": "always",
        "panel": "shared"
      },
      "problemMatcher": []
    },
    {
      "label": "Instalar todos os módulos (skip tests)",
      "type": "shell",
      "command": "mvn clean install -DskipTests -T 1",
      "options": {
        "cwd": "${workspaceFolder}/rsdesenvolvimento-api"
      },
      "group": "build",
      "presentation": {
        "reveal": "always",
        "panel": "shared"
      },
      "problemMatcher": []
    }
  ]
}
